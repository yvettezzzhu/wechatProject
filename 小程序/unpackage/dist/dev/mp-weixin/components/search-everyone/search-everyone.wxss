}
.box .second2 .yes .name {
  color: #6e8fff;
}
.box .second2 .yes .hid {
  color: #afafaf;
  margin-left: 8px;
}
.box .second2 .no {
  color: #fc4445;
}
.nav {
  margin-top: 8px;
  display: -webkit-box;
