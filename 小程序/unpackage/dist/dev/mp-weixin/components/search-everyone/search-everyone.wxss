.bottom {
  margin-top: 10px;
  padding: 0;
}
.bottom .box {
  display: inline-block;
  text-align: center;
}
.bottom .box navigator {
  background-color: #2C405A;
  width: 100%;
  position: relative;
}
.bottom .box navigator image {
  width: 100%;
  height: 150px;
  border-radius: 10rpx;
}
.bottom .box navigator .play-love {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 12px;
  color: #ffffff;
  height: 20px;
  position: absolute;
  bottom: 50px;
  left: 5px;
}
.bottom .box navigator .play-love .play {
  margin-right: 10rpx;
  margin-left: 6rpx;
}
.bottom .box navigator .play-love .love {
  margin-left: 6rpx;
}
.bottom .box navigator .title {
  width: 240rpx;
  height: 36px;
  font-size: 12px;
  word-break: keep-all;
  word-wrap: break-word;
  white-space: pre-line;
  overflow: hidden;
  text-align: left;
}
.process {
  margin-top: 8px;
}
.process .long {
  background-color: #007AFF;
  width: 100%;
  height: 16px;
  border-radius: 16px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.process .long .short1 {
  width: 30%;
  font-size: 10px;
  color: #ffffff;
  background-color: #6e8fff;
  border-bottom-left-radius: 16px;
  border-top-left-radius: 16px;
  text-indent: 8px;
}
.process .long .short2 {
  width: 70%;
  color: #ffffff;
  font-size: 10px;
  background-color: #fc4445;
  text-align: right;
  padding-right: 8px;
  border-bottom-right-radius: 16px;
  border-top-right-radius: 16px;
}
.box {
  background-color: #eff2f5;
  margin-top: 15px;
  margin-left: 30rpx;
  margin-right: 30rpx;
  padding: 30rpx;
}
.box .first2 {
  width: 100%;
